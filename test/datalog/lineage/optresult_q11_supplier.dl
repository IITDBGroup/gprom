PROV_SUPPLIER(PS_SK,S1,S2,S_NK,S3,S4,S5) :-
    SUPPLIER(PS_SK,S1,S2,S_NK,S3,S4,S5),
    PARTSUPP(PS_PK,PS_SK,PS_AQ,PS_SC,PS2),
    NATION(S_NK,'GERMANY',N1,N2),
    RTPCQ11(PS_PK,V1).

PROV_SUPPLIER(PS_SK,S1,S2,S_NK,S3,S4,S5) :-
    SUPPLIER(PS_SK,S1,S2,S_NK,S3,S4,S5),
    PARTSUPP(PS_PK,PS_SK,PS_AQ,PS_SC,PS2),
    NATION(S_NK,'GERMANY',N1,N2),
    QSUB1(PS_PK,V1),
    QSUB1(C1,C2),
    QSUB2(V0),
    ((C2 > V0)),
    RTPCQ11(C1,C2).

QSUB1(PS_PK,sum((PS_SC * PS_AQ))) :-
    PARTSUPP(PS_PK,PS_SK,PS_AQ,PS_SC,PS2),
    SUPPLIER(PS_SK,S1,S2,S_NK,S3,S4,S5),
    NATION(S_NK,'GERMANY',N1,N2).

QSUB2(sum((C2 * 0.000100))) :-
    QSUB1(C1,C2).


ANS: PROV_SUPPLIER.
