PROV_ORDERS(O_ORDERKEY,C_CUSTKEY,O1,O2,O3,O4,O5,O6,O7) :-
    ORDERS(O_ORDERKEY,C_CUSTKEY,O1,O2,O3,O4,O5,O6,O7),Q1(C_CUSTKEY,V1),RTPCQ13(V1,V0).

Q1(C_CUSTKEY,count(O_ORDERKEY)) :-
