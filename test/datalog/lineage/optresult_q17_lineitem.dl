PROV_LINEITEM(L1,L_PK,L2,L3,L_Q,L_EP,L4,L5,L6,L7,L8,L9,L10,L11,L12,L13) :-
    LINEITEM(L1,L_PK,L2,L3,L_Q,L_EP,L4,L5,L6,L7,L8,L9,L10,L11,L12,L13),
    QSUB(L_PK,Q2),
    PART(L_PK,P1,P2,'Brand#23',P3,P4,'MED BOX',P5,P6),
    ((L_Q < Q2)),
    RTPCQ17(V1).

PROV_LINEITEM(L1,L_PK,L2,L3,L_Q,L_EP,L4,L5,L6,L7,L8,L9,L10,L11,L12,L13) :-
    LINEITEM(L1,L_PK,L2,L3,L_Q,L_EP,L4,L5,L6,L7,L8,L9,L10,L11,L12,L13),
    QSUB(L_PK,V1),
    PART(L_PK,P1,P2,'Brand#23',P3,P4,'MED BOX',P5,P6),
    LINEITEM(V2,L_PK,V3,V4,V5,V6,V7,V8,V9,V10,V11,V12,V13,V14,V15,V16),
    ((V5 < V1)),
    RTPCQ17(V17).

QSUB(L_PK,avg((0.200000 * L_Q))) :-
    LINEITEM(L1,L_PK,L2,L3,L_Q,L_EP,L4,L5,L6,L7,L8,L9,L10,L11,L12,L13).

ANS: PROV_LINEITEM.
